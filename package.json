{
  "name": "Vuedux",
  "version": "0.0.0",
  "description": "Redux bindings for Vue.js, inspired by React-Redux.",
  "author": "jaredramirez <jaredramirez@me.com>",
  "license": "MIT",
  "entry": "index.js",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "lint": "node_modules/.bin/eslint index.js src/*.js",
